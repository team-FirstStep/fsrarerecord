<h4>配送先の追加</h4>
            <%= form_for @add_address do |add| %>
            <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label>名前(姓)</label><br>
                        <%= add.text_field :add_last_name %>
                        <br>
                        <label>名前(セイ)</label><br>
                        <%= add.text_field :add_last_name_kana %>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="form-group">
                        <label>名前(名)</label><br>
                        <%= add.text_field :add_first_name %>
                        <br>
                        <label>名前(メイ)</label><br>
                        <%= add.text_field :add_first_name_kana %>
                    </div>
                </div>
            </div>
            <%#= f.fields_for :addresses do |address| %>
            <div class="form-group">
                <label>郵便番号</label><br>
                <%= add.text_field :add_zip %>
            </div>
            <div class="form-group">
                <label>住所</label><br>
                <%= add.text_field :add_address %>
            <%# end %>
            </div>
            <%= add.submit "追加する", class: "btn btn-default" %>
            <br>
            <% end %>
            <%= link_to "戻る", user_path(current_user), class: "btn btn-default" %>
        </div>
